  "description": "PHP Supported CMS Libraries",
  "homepage": "https://github.com/ezpizee/supported-cms",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Pang Khon",
      "email": "khon.pang@ezpizee.com"
    },
    {
      "name": "Sothea Nim",
      "email": "sothea.nim@ezpizee.com.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ezpizee/utils": "dev-master",
    "ezpizee/microservices-client": "dev-master"
